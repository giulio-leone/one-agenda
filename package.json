{
  "name": "@giulio-leone/one-agenda",
  "version": "0.15.0",
  "description": "Motore di pianificazione proattiva OneAgenda per onecoach",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "echo 'Skipping one-agenda build for web deployment'",
    "build:full": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@giulio-leone/lib-core": "workspace:*",
    "@giulio-leone/lib-shared": "workspace:*",
    "ai": "6.0.77",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.22",
    "node-ical": "^0.22.1",
    "uuid": "^13.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "bin": {
    "oneagenda": "bin/oneagenda.mjs"
  },
  "sideEffects": false
}
